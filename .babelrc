{
    "compact": true,
    "comments": false,
    "presets": [
        "@babel/typescript",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ],
        "minify"
    ],
    "ignore": ["**/*.d.ts", "**/__tests__"],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}
