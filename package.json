{
  "name": "@ccalamos/gatsby-source-googlemaps-static",
  "description": "Gatsby source plugin for Google Maps Static API",
  "version": "2.0.0",
  "author": "Cole Calamos <cole@colejcalamos.com>",
  "bugs": {
    "url": "https://github.com/ccalamos/gatsby-source-googlemaps-static/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "googlemaps",
    "google",
    "maps",
    "static"
  ],
  "homepage": "https://www.gatsbyjs.org/packages/@ccalamos/gatsby-source-googlemaps-static/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccalamos/gatsby-source-googlemaps-static.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "gatsby": "^2.31.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "fs-extra": "^9.1.0",
    "gatsby-source-filesystem": "^2.10.0",
    "query-string": "^6.13.8"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "clean": "rm -rf coverage && rm -rf dist && rm -rf types",
    "test:coverage": "jest --collect-coverage",
    "format": "prettier --write \"{,!(node_modules)/**/}*.ts\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepublishOnly": "cross-env NODE_ENV=production yarn run clean; cross-env NODE_ENV=production yarn run build",
    "test": "jest",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "watch": "tsc -p ./tsconfig.json -w"
  },
  "peerDependencies": {
    "gatsby": "^2.24.69"
  },
  "main": "dist/gatsby-node.js",
  "files": [
    "dist/*.js"
  ],
  "types": "index.d.ts",
  "engines": {
    "node": ">=10.13.0"
  }
}
